{
    "compilerOptions": {
        "target": "ES2021",
        "module": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "resolveJsonModule": true
    },
    "include": [
        "src"
    ]
}
